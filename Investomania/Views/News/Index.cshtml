@model IEnumerable<Investomania.DAL.New>


@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="breadcrumb Imgbreadcrumb">
    <!-- CONTAINER -->
    <div class="container">
        <h2>الاخبار</h2>
    </div><!-- //CONTAINER -->
</section>
<div class="breadcrumb-page">
    <nav class="breadcrumb">
        <a class="breadcrumb-item" href="/" title="Home">الرئيسيه /</a>
        <span class="breadcrumb-item active">الاخبار</span>
    </nav>
</div>
<section class="row">
    <div class="col-md-9">
         @if (Model != null)
         {
       foreach (var item in Model)
        {
        <div class="col-md-12">
            <div id="" class="row artsec">
              
                <div class="col-md-9 col-sm-8">
                    <div class="entry-details">
                        <h4 class="entry-title"><a href="#" rel="">@item.newsTitle</a></h4>
                        <div class="entry-content">
                            @item.newsContent
                                   </div>
                       
                    </div>
                </div>
            </div>
        </div>
        } 
         }
    </div>
    <div class="col-md-3 catygory">
        <h1>@Html.ActionLink(" + ", "Create", "News")</h1>
        <ul>
            <li> <a>cat1</a> </li>
            <li> <a>cat2</a> </li>
            <li> <a>cat3</a> </li>
            <li> <a>cat4</a> </li>
            <li> <a>cat5</a> </li>
        </ul>
    </div>
</section>

<div class="modal" tabindex="-1" role="dialog" id="modal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="modalHeader">Modal title</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" id="modalBody">
                <p>Modal body text goes here.</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary">Save changes</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>